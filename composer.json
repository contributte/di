{
  "name": "contributte/di",
  "description": "Extra contrib to nette/di",
  "keywords": [
    "nette",
    "dependency",
    "inject"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/di",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.1",
    "nette/di": "^3.1.0",
    "nette/utils": "^3.2.8 || ^4.0"
  },
  "require-dev": {
    "contributte/qa": "^0.4",
    "contributte/tester": "^0.3",
    "contributte/phpstan": "^0.1",
    "nette/robot-loader": "^3.4.2 || ^4.0",
    "nette/bootstrap": "^3.1.4"
  },
  "conflict": {
    "nette/schema": "<1.1.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\DI\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.6.x-dev"
    }
  }
}
